  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dominique-mueller/angular-notifier.git"
  },
  "homepage": "https://github.com/dominique-mueller/angular-notifier",
  "bugs": {
    "url": "https://github.com/dominique-mueller/angular-notifier/issues"
